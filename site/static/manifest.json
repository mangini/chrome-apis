{
  "manifest_version": 2,
  "name": "Chrome API search",
  "version": "2",
  "minimum_chrome_version": "23",
  "permissions": [ 
    "http://chrome-api.storage.googleapis.com/*",
    "https://www.google-analytics.com/",
    "storage",
    "fileSystem",
    "unlimitedStorage"
  ],
  "app": {
    "background": {
      "scripts": ["main.js"]
    }
  }
}
